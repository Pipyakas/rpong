{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug rpong",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/rpong.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "C:/raylib/w64devkit/bin/gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Build for Windows",
      "internalConsoleOptions": "openOnSessionStart",
      "logging": {
        "engineLogging": true
      }
    }
  ]
}